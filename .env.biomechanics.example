# ==================================
# Environment Variables - NFC/NFV Biomechanics
# Copie para .env.biomechanics e configure
# ==================================

# -----------------------
# Database (PostgreSQL)
# -----------------------
POSTGRES_USER=nfc
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=nfc_biomechanics
POSTGRES_PORT=5432
DATABASE_URL=postgresql://nfc:your_secure_password_here@localhost:5432/nfc_biomechanics

# -----------------------
# Redis
# -----------------------
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_COMMANDER_PORT=8081

# -----------------------
# TensorFlow Configuration
# -----------------------
TF_BACKEND=cpu              # cpu | wasm | gpu
TF_THREADS=4                # Número de threads CPU

# -----------------------
# Video Processing
# -----------------------
MAX_FPS=30                  # FPS máximo de extração
BATCH_SIZE=10               # Frames por batch
MAX_CONCURRENT_VIDEOS=2     # Vídeos simultâneos

# Video Limits
MAX_VIDEO_DURATION=60       # Segundos
MAX_VIDEO_SIZE=104857600    # Bytes (100MB)

# -----------------------
# Upload
# -----------------------
UPLOAD_DIR=./data/videos/uploads
TEMP_DIR=./data/temp

# -----------------------
# Application
# -----------------------
NODE_ENV=production
APP_PORT=3000
PORT=3000

# CORS
CORS_ORIGINS=http://localhost:3000,https://chat.nutrifitcoach.com.br

# -----------------------
# Worker
# -----------------------
WORKER_CONCURRENCY=2
WORKER_CPU_LIMIT=2
WORKER_MEM_LIMIT=4G
WORKER_CPU_RESERVE=1
WORKER_MEM_RESERVE=2G

# -----------------------
# Tools (Development)
# -----------------------
BULLMQ_BOARD_PORT=8082

# -----------------------
# Monitoring (Opcional)
# -----------------------
SENTRY_DSN=
LOG_LEVEL=info

# -----------------------
# Webhooks (Opcional)
# -----------------------
WEBHOOK_TIMEOUT=5000        # ms
WEBHOOK_RETRY_ATTEMPTS=3
